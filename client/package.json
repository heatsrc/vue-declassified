{
  "name": "@heatsrc/vue-declassified",
  "version": "3.2.3",
  "description": "Transform Vue Class Components to Vue 3 script setup",
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=18.18.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/main.cjs.js",
  "module": "./dist/main.es.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "build": "rimraf dist/* && vite build",
    "lint": "tsc --noEmit && prettier --check ./**/*.{ts,json,vue}"
  },
  "keywords": [
    "vue-declassified",
    "vue declassified",
    "declassify",
    "vuedc",
    "vue-class-component",
    "vue class component",
    "vue-property-decorator",
    "vue property decorator",
    "vuex-class",
    "vuex class",
    "composition api",
    "composition",
    "script setup",
    "setup api",
    "vue",
    "vue 3",
    "migration",
    "transform",
    "cli"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heatsrc/vue-declassified.git",
    "directory": "client"
  },
  "author": "Jared McAteer <jared.mcateer@heatsrc.com>",
  "license": "ISC",
  "peerDependencies": {
    "@ts-morph/bootstrap": "^0.21.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.10",
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "^0.34.5",
    "rimraf": "^5.0.5",
    "ts-clone-node": "^3.0.0",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.4",
    "vue-class-component": "8.0.0-rc.1",
    "vue-property-decorator": "10.0.0-rc.3"
  },
  "dependencies": {
    "debug": "^4.3.4"
  }
}
