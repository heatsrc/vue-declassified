{
  "name": "@heatsrc/vuedc",
  "description": "Transform Vue Class Components to Vue 3 script setup",
  "version": "3.3.2",
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">= 18.18.0"
  },
  "type": "commonjs",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/src/vuedc.js",
  "bin": {
    "vuedc": "dist/src/vuedc.js"
  },
