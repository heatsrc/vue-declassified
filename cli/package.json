  "author": "Jared McAteer <jared.mcateer@heatsrc.com>",
  "license": "ISC",
  "dependencies": {
    "@heatsrc/vue-declassified": "workspace:^",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "figlet": "^1.6.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.6",
    "@types/node": "^20.8.4",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
